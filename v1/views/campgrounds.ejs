<% include partials/header %>

<div class="container">
<header class="jumbotron">
    <div class="container">
        <h1 class="display-4">Hello There! It's nice to have you here!</h1>
        <p class="lead">Check out amazing hand-pick places from all over the world!</p>
        <a class="btn btn-lg btn-primary" href="/campgrounds/new">Create a new campground</a>
    </div>
</header>
<div class="row">
<% campgrounds.forEach(function(campground){ %>
<div class="col-sm">
<h2> <%= campground.name %> </h2>
<img src="<%= campground.image %>">
</div>
<% }); %>
</div>
</div>

<% include partials/footer %>
